<?xml version="1.0" encoding="UTF-8"?>
<root BTCPP_format="4">
  <BehaviorTree ID="big_blue">
     <Sequence>
      <ScoreboardTask points="5"
                      task="basket"/>
      <JointPosition instance="k1"
                     max_effort="0"
                     max_velocity="120"
                     position="99"
                     tolerance="10"
                     feedback_effort="feedback_effort"
                     feedback_position="feedback_position"
                     result="result"/>
      <WaitMatchStart state="2"/>
      <Timeout msec="100000">
        <ForceSuccess>
          <Sequence>
            <ForceSuccess>
              <Timeout msec="85000">
                <TaskSequence>
                  <ForceSuccess>
                    <SubTree ID="skill_take_cherry_blue_start"
                             color="{color}"
                             node="{node}"
                             table="{table}"/>
                  </ForceSuccess>
                  <ForceSuccess>
                    <SubTree ID="skill_basket_blue"
                             color="{color}"
                             node="{node}"
                             table="{table}"/>
                  </ForceSuccess>
                  <ForceSuccess>
                    <Sequence>
                      <Translate goal="0.12"
                                 max_acceleration="0.5"
                                 max_velocity="0.8"/>
                      <SubTree ID="move_wait"
                               color="{color}"
                               goal="-0.40;1.23;0"
                               node="{node}"
                               table="{table}"/>
                      <SubTree ID="skill_take_side_cherry"
                               color="{color}"
                               node="{node}"
                               table="{table}"/>
                      <Translate goal="-0.5"
                                 max_acceleration="0.5"
                                 max_velocity="0.8"/>
                      <Wait duration="25"/>
                      <ForceSuccess>
                        <SubTree ID="skill_basket_blue"
                                 color="{color}"
                                 node="{node}"
                                 table="{table}"/>
                      </ForceSuccess>
                    </Sequence>
                  </ForceSuccess>
                  <SubTree ID="move_wait"
                           color="{color}"
                           goal="-0.40;1.23;0"
                           node="{node}"
                           table="{table}"/>
                </TaskSequence>
              </Timeout>
            </ForceSuccess>
            <CanbusSend can_id="27650"
                        message="0"/>
            <Navigate behavior_tree=""
                      goal="-0.72; -0.10;90"/>
            <CanbusSend can_id="27651"
                        message="1"/>
          </Sequence>
        </ForceSuccess>
      </Timeout>
      <CanbusSend can_id="27651"
                  message="1"/>
      <ScoreboardTask points="5"
                      task="funny_action"/>
      <ScoreboardTask points="15"
                      task="funny_action"/>
    </Sequence>
  </BehaviorTree>

  <!-- Description of Node Models (used by Groot) -->
  <TreeNodesModel>
    <Action ID="CanbusSend"
            editable="true">
      <input_port name="can_id">For example, 0x12345</input_port>
      <input_port name="message">For example, 0x32;0x34</input_port>
    </Action>
    <Control ID="TaskSequence"
             editable="true"/>
    <Action ID="Wait"
            editable="true">
      <input_port name="duration"/>
    </Action>
  </TreeNodesModel>

</root>
