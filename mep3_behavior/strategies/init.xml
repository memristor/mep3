                         feedback_effort="{feedback_effort}"
                         feedback_position="{feedback_position}"
                         result="{result}"/>
        </Sequence>
      </Parallel>
    </Sequence>
  </BehaviorTree>

  <BehaviorTree ID="init_up">
    <Sequence>
      <Parallel failure_count="1"
                success_count="-1">
        <Sequence>
          <JointPosition instance="m4"
                         max_effort="0"
                         max_velocity="180"
                         position="300"
                         tolerance="10"
                         feedback_effort="{feedback_effort}"
                         feedback_position="{feedback_position}"
                         result="{result}"/>
          <JointPosition instance="m3"
                         max_effort="0"
                         max_velocity="220"
                         position="85"
                         tolerance="10"
                         feedback_effort="{feedback_effort}"
