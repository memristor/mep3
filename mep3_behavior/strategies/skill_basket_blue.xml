<?xml version="1.0" encoding="UTF-8"?>
<root BTCPP_format="4">
  <BehaviorTree ID="skill_basket_blue">
    <Sequence>
      <RetryUntilSuccessful num_attempts="6">
        <Fallback>
          <Navigate behavior_tree=""
                    goal="-0.36;0.90;-90"/>
          <Sequence>
            <Wait duration="0.5"/>
            <AlwaysFailure/>
          </Sequence>
        </Fallback>
      </RetryUntilSuccessful>
      <SubTree ID="move_wait"
               color="{color}"
               goal="-0.56;1.25;-90"
               node="{node}"
               table="{table}"
               _autoremap="false"/>
      <ForceSuccess>
        <SubTree ID="move_wait"
                 color="{color}"
                 goal="-0.56;1.43;-90"
                 node="{node}"
                 table="{table}"
                 _autoremap="false"/>
      </ForceSuccess>
      <Sequence>
        <ForceSuccess>
          <CanbusSend can_id="27650"
                      message="5"/>
        </ForceSuccess>
        <JointPosition instance="k1"
                       max_effort="0"
                       max_velocity="100"
                       position="154"
                       tolerance="7"
                       feedback_effort="feedback_effort"
                       feedback_position="feedback_position"
                       result="result"/>
        <Wait duration="4"/>
        <CanbusSend can_id="27650"
                    message="0"/>
        <Parallel failure_count="1"
                  success_count="-1">
          <Translate goal="0.10"
                     max_acceleration="0.5"
                     max_velocity="1.2"/>
          <JointPosition instance="k1"
                         max_effort="0"
                         max_velocity="100"
                         position="99"
                         tolerance="10"
                         feedback_effort="feedback_effort"
                         feedback_position="feedback_position"
                         result="result"/>
        </Parallel>
      </Sequence>
    </Sequence>
  </BehaviorTree>

  <!-- Description of Node Models (used by Groot) -->
  <TreeNodesModel>
    <Action ID="CanbusSend"
            editable="true">
      <input_port name="can_id">For example, 0x12345</input_port>
      <input_port name="message">For example, 0x32;0x34</input_port>
    </Action>
    <Action ID="JointPosition"
            editable="true">
      <input_port name="instance"/>
      <input_port name="max_effort"
                  default="0"/>
      <input_port name="max_velocity"
                  default="180"/>
      <input_port name="position"/>
      <input_port name="tolerance"
                  default="10"/>
      <output_port name="feedback_effort"
                   default="feedback_effort"/>
      <output_port name="feedback_position"
                   default="feedback_position"/>
      <output_port name="result"
                   default="result"/>
    </Action>
    <Action ID="Navigate"
            editable="true">
      <input_port name="behavior_tree"/>
      <input_port name="goal"/>
    </Action>
    <Action ID="Translate"
            editable="true">
      <input_port name="goal"
                  default="0"/>
      <input_port name="max_acceleration"
                  default="0.5"/>
      <input_port name="max_velocity"
                  default="0.8"/>
    </Action>
    <Action ID="Wait"
            editable="true">
      <input_port name="duration"/>
    </Action>
  </TreeNodesModel>

</root>
