<?xml version="1.0" encoding="UTF-8"?>
<root BTCPP_format="4">
  <BehaviorTree ID="skill_basket_blue">
    <Sequence>
      <SubTree ID="move_wait"
               color="{color}"
               goal="-0.748;1.38;-90"
               node="{node}"
               table="{table}"/>
      <CanbusSend can_id="27650"
                  message="3"/>
      <Wait name="Otvori"
            duration="0.1"/>
      <Translate goal="0.1"
                 max_acceleration="0.5"
                 max_velocity="1.2"/>
    </Sequence>
  </BehaviorTree>

  <!-- Description of Node Models (used by Groot) -->
  <TreeNodesModel>
    <Action ID="CanbusSend"
            editable="true">
      <input_port name="can_id">For example, 0x12345</input_port>
      <input_port name="message">For example, 0x32;0x34</input_port>
    </Action>
    <Action ID="Translate"
            editable="true">
      <input_port name="goal"
                  default="0"/>
      <input_port name="max_acceleration"
                  default="0.5"/>
      <input_port name="max_velocity"
                  default="0.8"/>
    </Action>
    <Action ID="Wait"
            editable="true">
      <input_port name="duration"/>
    </Action>
  </TreeNodesModel>

</root>
