<?xml version="1.0" encoding="UTF-8"?>
<root BTCPP_format="4">
  <BehaviorTree ID="test_servo">
    <Sequence>
      <Parallel failure_count="1"
                success_count="-1">
        <JointPosition instance="joint_cherry_vertical"
                       max_velocity="200"
                       position="267"
                       tolerance="10"/>
        <JointPosition instance="joint_left_right"
                       max_velocity="200"
                       position="80"
                       tolerance="10"/>
        <JointPosition instance="joint_cherry_gripper"
                       max_velocity="200"
                       position="152"
                       tolerance="10"/>
        <JointPosition instance="joint_cake_left_gripper"
                       max_velocity="200"
                       position="150"
                       tolerance="10"/>
        <JointPosition instance="joint_cake_right_gripper"
                       max_velocity="200"
                       position="200"
                       tolerance="10"/>
      </Parallel>
      <Parallel failure_count="1"
                success_count="-1">
        <Parallel failure_count="1"
                  success_count="-1">
          <JointPosition instance="joint_cherry_vertical"
                         max_velocity="300"
                         position="267"
                         tolerance="10"/>
          <JointPosition instance="joint_left_right"
                         max_velocity="100"
                         position="90"
                         tolerance="10"/>
          <JointPosition instance="joint_cherry_gripper"
                         max_velocity="100"
                         position="152"
                         tolerance="10"/>
        </Parallel>
        <Sequence>
          <Parallel failure_count="1"
                    success_count="-1">
            <JointPosition instance="joint_cake_left_gripper"
                           max_velocity="100"
                           position="185"
                           tolerance="10"/>
            <JointPosition instance="joint_cake_right_gripper"
                           max_velocity="100"
                           position="200"
                           tolerance="10"/>
          </Parallel>
          <JointPosition instance="joint_cake_vertical"
                         max_velocity="100"
                         position="180"
                         tolerance="10"/>
          <Wait name="PeekCake"
                duration="5"/>
          <Parallel failure_count="1"
                    success_count="-1">
            <JointPosition instance="joint_cake_right_gripper"
                           max_velocity="100"
                           position="94"
                           tolerance="10"/>
            <JointPosition instance="joint_cherry_vertical"
                           max_velocity="100"
                           position="189"
                           tolerance="10"/>
          </Parallel>
          <JointPosition instance="joint_left_right"
                         max_velocity="300"
                         position="247"
                         tolerance="10"/>
          <Wait name="ActivatePump"
                duration="0.5"/>
          <JointPosition instance="joint_left_right"
                         max_velocity="300"
                         position="105"
                         tolerance="10"/>
          <JointPosition instance="joint_cherry_vertical"
                         max_velocity="100"
                         position="268"
                         tolerance="10"/>
          <JointPosition instance="joint_left_right"
                         max_velocity="300"
                         position="247"
                         tolerance="10"/>
        </Sequence>
      </Parallel>
    </Sequence>
  </BehaviorTree>

  <!-- Description of Node Models (used by Groot) -->
  <TreeNodesModel>
    <Action ID="JointPosition"
            editable="true">
      <input_port name="instance"/>
      <input_port name="max_velocity"
                  default="180"/>
      <input_port name="position"/>
      <input_port name="tolerance"
                  default="10"/>
    </Action>
    <Action ID="Wait"
            editable="true">
      <input_port name="duration"/>
    </Action>
  </TreeNodesModel>

</root>
