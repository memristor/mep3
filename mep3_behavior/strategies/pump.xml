<?xml version="1.0" encoding="UTF-8"?>
<root BTCPP_format="4">
  <BehaviorTree ID="pump_pickup_back">
    <Sequence>
      <Pump connect="1"
            instance="pump1"/>
      <JointPosition instance="m62"
                     max_effort="0"
                     max_velocity="250"
                     position="280"
                     tolerance="10"
                     feedback_effort="{feedback_effort}"
                     feedback_position="{feedback_position}"
                     result="{result}"
                     command_mode="0"/>
      <JointPosition instance="m62"
                     max_effort="0"
                     max_velocity="250"
                     position="830"
                     tolerance="10"
                     feedback_effort="{feedback_effort}"
                     feedback_position="{feedback_position}"
                     result="{result}"
                     command_mode="0"/>
      <JointPosition instance="m40"
                     max_effort="0"
                     max_velocity="200"
                     position="105"
                     tolerance="10"
                     feedback_effort="{feedback_effort}"
                     feedback_position="{feedback_position}"
                     result="{result}"
                     command_mode="0"/>
    </Sequence>
  </BehaviorTree>

  <BehaviorTree ID="pump_pickup_front">
    <Sequence>
      <Pump connect="1"
            instance="pump2"/>
      <JointPosition instance="m52"
                     max_effort="0"
                     max_velocity="250"
                     position="-340"
                     tolerance="10"
                     feedback_effort="{feedback_effort}"
                     feedback_position="{feedback_position}"
                     result="{result}"
                     command_mode="0"/>
      <JointPosition instance="m52"
                     max_effort="0"
                     max_velocity="250"
                     position="220"
                     tolerance="10"
                     feedback_effort="{feedback_effort}"
                     feedback_position="{feedback_position}"
                     result="{result}"
                     command_mode="0"/>
    </Sequence>
  </BehaviorTree>

  <BehaviorTree ID="pump_release_back">
    <Sequence>
      <JointPosition instance="m62"
                     max_effort="0"
                     max_velocity="250"
                     position="730"
                     tolerance="10"
                     feedback_effort="{feedback_effort}"
                     feedback_position="{feedback_position}"
                     result="{result}"
                     command_mode="0"/>
      <Pump connect="0"
            instance="pump1"/>
      <JointPosition instance="m62"
                     max_effort="0"
                     max_velocity="250"
                     position="790"
                     tolerance="10"
                     feedback_effort="{feedback_effort}"
                     feedback_position="{feedback_position}"
                     result="{result}"
                     command_mode="0"/>
    </Sequence>
  </BehaviorTree>

  <BehaviorTree ID="pump_release_front">
    <Sequence>
      <JointPosition instance="m52"
                     max_effort="0"
                     max_velocity="250"
                     position="90"
                     tolerance="10"
                     feedback_effort="{feedback_effort}"
                     feedback_position="{feedback_position}"
                     result="{result}"
                     command_mode="0"/>
      <Pump connect="0"
            instance="pump2"/>
      <JointPosition instance="m52"
                     max_effort="0"
                     max_velocity="250"
                     position="190"
                     tolerance="10"
                     feedback_effort="{feedback_effort}"
                     feedback_position="{feedback_position}"
                     result="{result}"
                     command_mode="0"/>
    </Sequence>
  </BehaviorTree>

  <!-- Description of Node Models (used by Groot) -->
  <TreeNodesModel>
    <Action ID="JointPosition"
            editable="true">
      <input_port name="instance"/>
      <input_port name="max_effort"
                  default="0"/>
      <input_port name="max_velocity"
                  default="180"/>
      <input_port name="position"/>
      <input_port name="tolerance"
                  default="10"/>
      <output_port name="feedback_effort"
                   default="{feedback_effort}"/>
      <output_port name="feedback_position"
                   default="{feedback_position}"/>
      <output_port name="result"
                   default="{result}"/>
      <input_port name="command_mode"
                  default="0"/>
    </Action>
    <Action ID="Pump"
            editable="true">
      <input_port name="connect"
                  default="1"/>
      <input_port name="instance"/>
    </Action>
  </TreeNodesModel>

</root>
