<?xml version="1.0" encoding="UTF-8"?>
<root BTCPP_format="4">
  <BehaviorTree ID="blue_new_base_s">
    <TaskSequence>
      <ForceSuccess>
        <Sequence>
          <JointPosition instance="m15"
                         max_effort="0"
                         max_velocity="220"
                         position="250"
                         tolerance="10"
                         feedback_effort="{feedback_effort}"
                         feedback_position="{feedback_position}"
                         result="{result}"/>
          <RetryUntilSuccessful num_attempts="8">
            <Fallback>
              <Move goal="0.79;0.25;-90"
                    ignore_obstacles="true"
                    linear_velocity="1.2"
                    frame_id="map"
                    angular_velocity="1.0"
                    mode="7"
                    linear_acceleration="1.0"
                    angular_acceleration="1.8"
                    reversing="2"/>
              <Sequence>
                <Sleep msec="500"/>
                <AlwaysFailure/>
              </Sequence>
            </Fallback>
          </RetryUntilSuccessful>
        </Sequence>
      </ForceSuccess>
      <ForceSuccess>
        <Sequence>
          <Parallel failure_count="1"
                    success_count="-1">
            <JointPosition instance="m15"
                           max_effort="0"
                           max_velocity="220"
                           position="0"
                           tolerance="10"
                           feedback_effort="{feedback_effort}"
                           feedback_position="{feedback_position}"
                           result="{result}"/>
            <JointPosition instance="m14"
                           max_effort="0"
                           max_velocity="220"
                           position="250"
                           tolerance="10"
                           feedback_effort="{feedback_effort}"
                           feedback_position="{feedback_position}"
                           result="{result}"/>
          </Parallel>
          <RetryUntilSuccessful num_attempts="8">
            <Fallback>
              <Move name="MoveWeed#2"
                    goal="55"
                    ignore_obstacles="false"
                    linear_velocity="1.5"
                    frame_id="map"
                    angular_velocity="3.5"
                    mode="1"
                    linear_acceleration="1.0"
                    angular_acceleration="3.5"
                    reversing="2"/>
              <Sequence>
                <Sleep msec="500"/>
                <AlwaysFailure/>
              </Sequence>
            </Fallback>
          </RetryUntilSuccessful>
          <RetryUntilSuccessful num_attempts="3">
            <Fallback>
              <Move goal="0.30;-0.2;-90"
                    ignore_obstacles="true"
                    linear_velocity="1.8"
                    frame_id="map"
                    angular_velocity="1.0"
                    mode="6"
                    linear_acceleration="1.4"
                    angular_acceleration="1.8"
                    reversing="2"/>
              <Sequence>
                <Sleep msec="500"/>
                <AlwaysFailure/>
              </Sequence>
            </Fallback>
          </RetryUntilSuccessful>
          <JointPosition instance="m12"
                         max_effort="0"
                         max_velocity="220"
                         position="85"
                         tolerance="10"
                         feedback_effort="{feedback_effort}"
                         feedback_position="{feedback_position}"
                         result="{result}"/>
          <JointPosition instance="m14"
                         max_effort="0"
                         max_velocity="220"
                         position="180"
                         tolerance="10"
                         feedback_effort="{feedback_effort}"
                         feedback_position="{feedback_position}"
                         result="{result}"/>
          <Move name="MoveWeed#2"
                goal="90"
                ignore_obstacles="false"
                linear_velocity="1.5"
                frame_id="map"
                angular_velocity="3.5"
                mode="1"
                linear_acceleration="1.0"
                angular_acceleration="3.5"
                reversing="2"/>
          <Move goal="0.30;-0.75;90"
                ignore_obstacles="true"
                linear_velocity="0.5"
                frame_id="map"
                angular_velocity="1.0"
                mode="6"
                linear_acceleration="0.8"
                angular_acceleration="1.8"
                reversing="2"/>
          <JointPosition instance="m11"
                         max_effort="0"
                         max_velocity="220"
                         position="214"
                         tolerance="10"
                         feedback_effort="{feedback_effort}"
                         feedback_position="{feedback_position}"
                         result="{result}"/>
        </Sequence>
      </ForceSuccess>
      <ForceSuccess>
        <Sequence>
          <SubTree ID="gentle_attack"
                   type="7"
                   linear_velocity="1.2"
                   linear_acceleration="1.0"
                   goal="-0.30;-0.84;90"/>
          <JointPosition instance="m2"
                         max_effort="0"
                         max_velocity="180"
                         position="78"
                         tolerance="10"
                         feedback_effort="{feedback_effort}"
                         feedback_position="{feedback_position}"
                         result="{result}"/>
          <JointPosition instance="m3"
                         max_effort="0"
                         max_velocity="220"
                         position="215"
                         tolerance="10"
                         feedback_effort="{feedback_effort}"
                         feedback_position="{feedback_position}"
                         result="{result}"/>
          <SubTree ID="gentle_attack"
                   type="7"
                   linear_velocity="1.2"
                   linear_acceleration="1.0"
                   goal="-0.30;-0.65;90"/>
          <SubTree ID="try_pick_up_weed_front"
                   _autoremap="true"/>
        </Sequence>
      </ForceSuccess>
      <Move goal="-0.75;-0.765;0"
            ignore_obstacles="true"
            linear_velocity="0.5"
            frame_id="map"
            angular_velocity="1.0"
            mode="7"
            linear_acceleration="0.8"
            angular_acceleration="1.8"
            reversing="2"/>
      <ForceSuccess>
        <Timeout msec="1500">
          <Move goal="-0.91;-0.765;0"
                ignore_obstacles="true"
                linear_velocity="0.5"
                frame_id="map"
                angular_velocity="1.0"
                mode="2"
                linear_acceleration="0.8"
                angular_acceleration="1.8"
                reversing="2"/>
        </Timeout>
      </ForceSuccess>
      <SubTree ID="try_put_plenter_back_down_new"
               _autoremap="true"/>
    </TaskSequence>
  </BehaviorTree>

  <!-- Description of Node Models (used by Groot) -->
  <TreeNodesModel>
    <Action ID="JointPosition"
            editable="true">
      <input_port name="instance"/>
      <input_port name="max_effort"
                  default="0"/>
      <input_port name="max_velocity"
                  default="180"/>
      <input_port name="position"/>
      <input_port name="tolerance"
                  default="10"/>
      <output_port name="feedback_effort"
                   default="{feedback_effort}"/>
      <output_port name="feedback_position"
                   default="{feedback_position}"/>
      <output_port name="result"
                   default="{result}"/>
    </Action>
    <Action ID="Move"
            editable="true">
      <input_port name="goal"/>
      <inout_port name="ignore_obstacles"
                  default="false"/>
      <input_port name="linear_velocity"
                  default="0.5"/>
      <input_port name="frame_id"
                  default="map"/>
      <input_port name="angular_velocity"
                  default="1.0"/>
      <input_port name="mode"
                  default="7"/>
      <input_port name="linear_acceleration"
                  default="0.8"/>
      <input_port name="angular_acceleration"
                  default="1.8"/>
      <input_port name="reversing"
                  default="0"/>
    </Action>
    <Control ID="TaskSequence"
             editable="true"/>
  </TreeNodesModel>

</root>
