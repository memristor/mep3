<?xml version="1.0" encoding="UTF-8"?>
<root BTCPP_format="4">
  <BehaviorTree ID="skill_take_cherry_green_start">
    <Sequence>
      <Navigate behavior_tree=""
                goal="0.23;1.08;90"/>
      <Sequence>
        <ForceSuccess>
          <CanbusSend can_id="27650"
                      message="250"/>
        </ForceSuccess>
        <Wait duration="0.2"/>
        <SubTree ID="move_cherry"
                 color="{color}"
                 goal="0.226;1.35;90"
                 node="{node}"
                 table="{table}"/>
        <Wait duration="2"/>
        <ForceSuccess>
          <CanbusSend can_id="27650"
                      message="0"/>
        </ForceSuccess>
      </Sequence>
  </BehaviorTree>

  <!-- Description of Node Models (used by Groot) -->
  <TreeNodesModel>
    <Action ID="CanbusSend"
            editable="true">
      <input_port name="can_id">For example, 0x12345</input_port>
      <input_port name="message">For example, 0x32;0x34</input_port>
    </Action>
    <Action ID="Move"
            editable="true">
      <input_port name="goal"/>
      <input_port name="ignore_obstacles"
                  default="true"/>
      <input_port name="max_acceleration"
                  default="0.5"/>
      <input_port name="max_velocity"
                  default="0.8"/>
    </Action>
    <Control ID="TaskSequence"
             editable="true"/>
    <Action ID="Wait"
            editable="true">
      <input_port name="duration"/>
    </Action>
  </TreeNodesModel>

</root>
