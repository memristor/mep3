<?xml version="1.0" encoding="UTF-8"?>
<root BTCPP_format="4">

  <BehaviorTree ID="banner">
    <Sequence>
      <SubTree ID="init_banner"
               _autoremap="true"/>
      <WaitMatchStart state="2"/>
      <Sleep msec="1500"/>
      <SubTree ID="pull_back_banner"
               _autoremap="true"/>
    </Sequence>
  </BehaviorTree>

  <BehaviorTree ID="init_banner">
    <Sequence>
      <Parallel failure_count="1"
                success_count="-1">
        <JointPosition instance="m21"
                       max_effort="0"
                       max_velocity="180"
                       position="47"
                       tolerance="10"
                       feedback_effort="{feedback_effort}"
                       feedback_position="{feedback_position}"
                       result="{result}"
                       command_mode="0"/>
        <JointPosition instance="m22"
                       max_effort="0"
                       max_velocity="180"
                       position="265"
                       tolerance="10"
                       feedback_effort="{feedback_effort}"
                       feedback_position="{feedback_position}"
                       result="{result}"
                       command_mode="0"/>
      </Parallel>
    </Sequence>
  </BehaviorTree>

  <BehaviorTree ID="pull_back_banner">
    <Sequence>
      <Parallel failure_count="1"
                success_count="-1">
        <JointPosition instance="m21"
                       max_effort="0"
                       max_velocity="180"
                       position="207"
                       tolerance="10"
                       feedback_effort="{feedback_effort}"
                       feedback_position="{feedback_position}"
                       result="{result}"
                       command_mode="0"/>
        <JointPosition instance="m22"
                       max_effort="0"
                       max_velocity="180"
                       position="105"
                       tolerance="10"
                       feedback_effort="{feedback_effort}"
                       feedback_position="{feedback_position}"
                       result="{result}"
                       command_mode="0"/>
      </Parallel>
    </Sequence>
  </BehaviorTree>



  <!-- Description of Node Models (used by Groot) -->
  <TreeNodesModel/>

</root>
