<?xml version="1.0" encoding="UTF-8"?>
<root BTCPP_format="4">
  <BehaviorTree ID="big_strategy">
    <Sequence>
      <Translate goal="0.5" />
      <Move goal="0;0;0"
            ignore_obstacles="true"/>
    </Sequence>
  </BehaviorTree>

  <!-- Description of Node Models (used by Groot) -->
  <TreeNodesModel>
    <Action ID="Move"
            editable="false">
      <input_port name="goal"/>
      <input_port name="ignore_obstacles"
                  default="true"/>
    </Action>
    <Action ID="Translate"
            editable="true">
      <input_port name="goal"
                  default="0"/>
      <input_port name="max_velocity"
                  default="0.5"/>
    </Action>
  </TreeNodesModel>

</root>
