<?xml version="1.0" encoding="UTF-8"?>
<root BTCPP_format="4">
  <BehaviorTree ID="big_strategy">
    <SequenceWithMemory>
      <SubTree ID="test_init_servo"
               color="{color}"
               table="{table}"
               _autoremap="true"/>
      <Parallel failure_count="1"
                success_count="-1">
        <SubTree ID="prepair_pick_cake"
                 color="{color}"
                 table="{table}"
                 _autoremap="true"/>
        <Move goal="-0.27;-0.47;0"
              ignore_obstacles="false"
              max_acceleration="0.5"
              max_velocity="0.8"/>
      </Parallel>
      <JointPosition instance="m10"
                     max_effort="0"
                     max_velocity="180"
                     position="183"
                     tolerance="10"
                     feedback_effort="feedback_effort"
                     feedback_position="feedback_position"
                     result="result"/>
      <SubTree ID="prepair_pick_cake"
               color="{color}"
               table="{table}"
               _autoremap="true"/>
      <SubTree ID="skill_lift_cake"
               color="{color}"
               table="{table}"/>
      <Move goal="-0.3;-0.83;180"
            ignore_obstacles="false"
            max_acceleration="0.5"
            max_velocity="0.8"/>
      <Move goal="-0.65;-0.83;180"
            ignore_obstacles="false"
            max_acceleration="0.5"
            max_velocity="0.8"/>
      <Parallel failure_count="1"
                success_count="-1">
        <JointPosition instance="m10"
                       max_effort="0"
                       max_velocity="180"
                       position="183"
                       tolerance="10"
                       feedback_effort="inner_feedback_effort"
                       feedback_position="inner_feedback_position"
                       result="inner_result"/>
        <JointPosition instance="m1"
                       max_effort="0"
                       max_velocity="180"
                       position="111"
                       tolerance="10"
                       feedback_effort="feedback_effort"
                       feedback_position="feedback_position"
                       result="result"/>
      </Parallel>
      <SubTree ID="skill_lift_cake_right"
               color="{color}"
               table="{table}"/>
      <Translate goal="-0.35"
                 max_acceleration="0.5"
                 max_velocity="0.8"/>
      <Move goal="-0.35;0.3;90"
            ignore_obstacles="false"
            max_acceleration="0.5"
            max_velocity="0.8"/>
      <JointPosition instance="m1"
                     max_effort="0"
                     max_velocity="180"
                     position="111"
                     tolerance="10"
                     feedback_effort="feedback_effort"
                     feedback_position="feedback_position"
                     result="result"/>
      <Move goal="-0.30;1.2;90"
            ignore_obstacles="false"
            max_acceleration="0.5"
            max_velocity="0.8"/>
      <Move goal="-0.65;1.2;180"
            ignore_obstacles="false"
            max_acceleration="0.5"
            max_velocity="0.8"/>
      <Parallel failure_count="1"
                success_count="-1">
        <JointPosition instance="m1"
                       max_effort="0"
                       max_velocity="180"
                       position="83"
                       tolerance="10"
                       feedback_effort="feedback_effort"
                       feedback_position="feedback_position"
                       result="result"/>
        <JointPosition instance="m10"
                       max_effort="0"
                       max_velocity="180"
                       position="210"
                       tolerance="10"
                       feedback_effort="feedback_effort"
                       feedback_position="feedback_position"
                       result="result"/>
      </Parallel>
      <Translate goal="-0.15"
                 max_acceleration="0.5"
                 max_velocity="0.8"/>
      <SubTree ID="skill_drop_cake"
               color="{color}"
               table="{table}"/>
      <Translate goal="-0.15"
                 max_acceleration="0.5"
                 max_velocity="0.8"/>
    </SequenceWithMemory>
  </BehaviorTree>

  <!-- Description of Node Models (used by Groot) -->
  <TreeNodesModel>
    <Action ID="JointPosition"
            editable="true">
      <input_port name="instance"/>
      <input_port name="max_effort"
                  default="0"/>
      <input_port name="max_velocity"
                  default="180"/>
      <input_port name="position"/>
      <input_port name="tolerance"
                  default="10"/>
      <output_port name="feedback_effort"
                   default="feedback_effort"/>
      <output_port name="feedback_position"
                   default="feedback_position"/>
      <output_port name="result"
                   default="result"/>
    </Action>
    <Action ID="Move"
            editable="true">
      <input_port name="goal"/>
      <input_port name="ignore_obstacles"
                  default="true"/>
      <input_port name="max_acceleration"
                  default="0.5"/>
      <input_port name="max_velocity"
                  default="0.8"/>
    </Action>
    <Action ID="Translate"
            editable="true">
      <input_port name="goal"
                  default="0"/>
      <input_port name="max_acceleration"
                  default="0.5"/>
      <input_port name="max_velocity"
                  default="0.8"/>
    </Action>
  </TreeNodesModel>

</root>
