                  </Sequence>
                </Fallback>
              </RetryUntilSuccessful>
            </Sequence>
          </Fallback>
          <ForceSuccess>
            <Timeout msec="1000">
              <Move goal="-0.91;0.79;0"
                    ignore_obstacles="true"
                    linear_velocity="0.5"
                    frame_id="map"
                    angular_velocity="1.0"
                    mode="2"
                    linear_acceleration="0.8"
                    angular_acceleration="1.8"
                    reversing="2"/>
            </Timeout>
          </ForceSuccess>
          <SubTree ID="try_put_plenter_back_down_new"
                   _autoremap="true"/>
          <ScoreboardTask points="12"
                          task="ALTERNATIVE_PLENTER_SIDE"/>
          <RetryUntilSuccessful num_attempts="7">
            <Fallback>
              <Move goal="-0.70;0.765;0"
                    ignore_obstacles="false"
                    linear_velocity="0.5"
                    frame_id="map"
                    angular_velocity="1.0"
                    mode="3"
                    linear_acceleration="0.8"
                    angular_acceleration="1.8"
                    reversing="0"/>
              <Sequence>
                <Sleep msec="500"/>
                <AlwaysFailure/>
              </Sequence>
            </Fallback>
          </RetryUntilSuccessful>
        </Sequence>
      </ForceSuccess>
      <ForceSuccess>
        <Sequence>
          <RetryUntilSuccessful num_attempts="7"
                                _skipIf="is_second==true || is_thirt==true">
            <Fallback>
              <Move goal="-0.70;0.765;0"
                    ignore_obstacles="false"
                    linear_velocity="0.5"
                    frame_id="map"
                    angular_velocity="1.0"
                    mode="7"
                    linear_acceleration="0.8"
                    angular_acceleration="1.8"
                    reversing="0"/>
              <Sequence>
                <Sleep msec="500"/>
                <AlwaysFailure/>
              </Sequence>
            </Fallback>
          </RetryUntilSuccessful>
          <SubTree ID="try_pick_up_pots_with_fork_new_yellow_alternative"
                   _autoremap="true"/>
          <Sequence _onSuccess="is_in_home:=true"
                    _onFailure="is_in_home:=false">
            <Translate frame_id="base_link"
                       ignore_obstacles="true"
                       x="-0.03"
                       error="{error}"
                       linear_velocity="0.5"
                       linear_acceleration="0.8"
                       enable_sensors="false"/>
            <Move goal="-90"
                  ignore_obstacles="true"
                  linear_velocity="1.5"
                  frame_id="map"
                  angular_velocity="2.5"
                  mode="1"
                  linear_acceleration="1.0"
                  angular_acceleration="2.2"
                  reversing="0"/>
            <ForceSuccess>
              <Timeout msec="1000">
                <Move goal="-0.45;1.44;90"
                      ignore_obstacles="true"
                      linear_velocity="0.5"
                      frame_id="map"
                      angular_velocity="1.0"
                      mode="2"
                      linear_acceleration="0.8"
                      angular_acceleration="1.8"
                      reversing="2"/>
              </Timeout>
            </ForceSuccess>
            <SubTree ID="try_put_plenter_back_up_new"
                     _autoremap="true"/>
            <ScoreboardTask points="16"
                            task="ALTERNATIVE_PLENTER1"/>
            <Sequence>
              <Translate frame_id="base_link"
                         ignore_obstacles="true"
                         x="0.15"
                         error="{error}"
                         linear_velocity="0.5"
                         linear_acceleration="0.8"
                         enable_sensors="false"/>
              <SubTree ID="try_put_field"
                       _skipIf="is_thirt==false"
                       _autoremap="true"/>
            </Sequence>
            <Move goal="90"
                  ignore_obstacles="true"
                  linear_velocity="0.5"
                  frame_id="map"
                  angular_velocity="1.0"
                  mode="1"
                  linear_acceleration="0.8"
                  angular_acceleration="1.8"
                  reversing="0"/>
            <Translate frame_id="base_link"
                       ignore_obstacles="true"
                       x="-0.15"
                       error="{error}"
                       linear_velocity="0.5"
                       linear_acceleration="0.8"
                       enable_sensors="false"/>
            <Move goal="55"
                  ignore_obstacles="true"
                  linear_velocity="0.5"
                  frame_id="map"
                  angular_velocity="1.0"
                  mode="1"
                  linear_acceleration="0.8"
                  angular_acceleration="1.8"
                  reversing="0"/>
            <JointPosition instance="m15"
                           max_effort="0"
                           max_velocity="220"
                           position="288"
                           tolerance="10"
                           feedback_effort="{feedback_effort}"
                           feedback_position="{feedback_position}"
                           result="{result}"/>
            <SubTree ID="go_home_yellow_new"
                     _skipIf="is_thirt==true"
                     _autoremap="true"/>
          </Sequence>
        </Sequence>
      </ForceSuccess>
      <Sleep msec="120000"/>
    </Sequence>
  </BehaviorTree>

  <!-- Description of Node Models (used by Groot) -->
  <TreeNodesModel>
    <Action ID="JointPosition"
            editable="true">
      <input_port name="instance"/>
      <input_port name="max_effort"
                  default="0"/>
      <input_port name="max_velocity"
                  default="180"/>
      <input_port name="position"/>
      <input_port name="tolerance"
                  default="10"/>
      <output_port name="feedback_effort"
                   default="{feedback_effort}"/>
      <output_port name="feedback_position"
                   default="{feedback_position}"/>
      <output_port name="result"
                   default="{result}"/>
    </Action>
    <Action ID="Move"
            editable="true">
      <input_port name="goal"/>
      <inout_port name="ignore_obstacles"
                  default="false"/>
      <input_port name="linear_velocity"
                  default="0.5"/>
      <input_port name="frame_id"
                  default="map"/>
      <input_port name="angular_velocity"
                  default="1.0"/>
      <input_port name="mode"
                  default="7"/>
      <input_port name="linear_acceleration"
                  default="0.8"/>
      <input_port name="angular_acceleration"
                  default="1.8"/>
      <input_port name="reversing"
                  default="0"/>
    </Action>
    <Action ID="ScoreboardTask"
            editable="true">
      <input_port name="points"
                  default="0">points scored, can be negative</input_port>
      <input_port name="task"
                  default="store_sample_to_work_shed">unique task name</input_port>
    </Action>
    <Action ID="Translate"
            editable="true">
      <input_port name="frame_id"
                  default="base_link"/>
      <input_port name="ignore_obstacles"
                  default="false"/>
      <input_port name="x"/>
      <input_port name="error"
                  default="{error}"/>
      <input_port name="linear_velocity"
                  default="0.5"/>
      <input_port name="linear_acceleration"
                  default="0.8"/>
      <input_port name="enable_sensors"
                  default="false"/>
    </Action>
  </TreeNodesModel>

</root>
