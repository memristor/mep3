<?xml version="1.0" encoding="UTF-8"?>
<root BTCPP_format="4">
  <BehaviorTree ID="try_put_plenter">
    <Sequence>
      <JointPosition instance="m1"
                     max_effort="0"
                     max_velocity="220"
                     position="220"
                     tolerance="10"
                     feedback_effort="{feedback_effort}"
                     feedback_position="{feedback_position}"
                     result="{result}"/>
      <JointPosition instance="m4"
                     max_effort="0"
                     max_velocity="180"
                     position="0"
                     tolerance="10"
                     feedback_effort="{feedback_effort}"
                     feedback_position="{feedback_position}"
                     result="{result}"/>
      <JointPosition instance="m1"
                     max_effort="0"
                     max_velocity="180"
                     position="201"
                     tolerance="10"
                     feedback_effort="{feedback_effort}"
                     feedback_position="{feedback_position}"
                     result="{result}"/>
      <JointPosition instance="m4"
                     max_effort="0"
                     max_velocity="180"
                     position="180"
                     tolerance="10"
                     feedback_effort="{feedback_effort}"
                     feedback_position="{feedback_position}"
                     result="{result}"/>
      <JointPosition instance="m1"
                     max_effort="0"
                     max_velocity="50"
                     position="215"
                     tolerance="10"
                     feedback_effort="{feedback_effort}"
                     feedback_position="{feedback_position}"
                     result="{result}"/>
      <JointPosition instance="m4"
                     max_effort="0"
                     max_velocity="180"
                     position="35"
                     tolerance="10"
                     feedback_effort="{feedback_effort}"
                     feedback_position="{feedback_position}"
                     result="{result}"/>
      <JointPosition instance="m4"
                     max_effort="0"
                     max_velocity="180"
                     position="100"
                     tolerance="10"
                     feedback_effort="{feedback_effort}"
                     feedback_position="{feedback_position}"
                     result="{result}"/>
      <Parallel failure_count="1"
                success_count="-1">
        <JointPosition instance="m1"
                       max_effort="0"
                       max_velocity="50"
                       position="230"
                       tolerance="10"
                       feedback_effort="{feedback_effort}"
                       feedback_position="{feedback_position}"
                       result="{result}"/>
        <JointPosition instance="m4"
                       max_effort="0"
                       max_velocity="180"
                       position="300"
                       tolerance="10"
                       feedback_effort="{feedback_effort}"
                       feedback_position="{feedback_position}"
                       result="{result}"/>
      </Parallel>
      <Parallel failure_count="1"
                success_count="-1">
        <JointPosition instance="m2"
                       max_effort="0"
                       max_velocity="180"
                       position="107"
                       tolerance="10"
                       feedback_effort="{feedback_effort}"
                       feedback_position="{feedback_position}"
                       result="{result}"/>
        <JointPosition instance="m3"
                       max_effort="0"
                       max_velocity="180"
                       position="184"
                       tolerance="10"
                       feedback_effort="{feedback_effort}"
                       feedback_position="{feedback_position}"
                       result="{result}"/>
      </Parallel>
      <Translate frame_id="base_link"
                 ignore_obstacles="true"
                 x="-0.1"
                 error="{error}"
                 linear_velocity="0.1"/>
    </Sequence>
  </BehaviorTree>

  <BehaviorTree ID="try_put_plenter_back">
    <Sequence>
      <Move goal="-0.50;-0.7;0"
            ignore_obstacles="false"
            linear_velocity="1.0"
            frame_id="map"
            angular_velocity="1.8"
            mode="3"/>
      <Move goal="-0.94;-0.7;0"
            ignore_obstacles="false"
            linear_velocity="1.0"
            frame_id="map"
            angular_velocity="1.8"
            mode="3"/>
      <Move goal="-0.945;-0.7;0"
            ignore_obstacles="false"
            linear_velocity="0.3"
            frame_id="map"
            angular_velocity="1.8"
            mode="3"/>
      <ScoreboardTask points="10"
                      task="test"/>
      <JointPosition instance="m11"
                     max_effort="0"
                     max_velocity="180"
                     position="150"
                     tolerance="10"
                     feedback_effort="{feedback_effort}"
                     feedback_position="{feedback_position}"
                     result="{result}"/>
      <JointPosition instance="m14"
                     max_effort="0"
                     max_velocity="190"
                     position="180"
                     tolerance="10"
                     feedback_effort="{feedback_effort}"
                     feedback_position="{feedback_position}"
                     result="{result}"/>
      <JointPosition instance="m11"
                     max_effort="0"
                     max_velocity="180"
                     position="210"
                     tolerance="10"
                     feedback_effort="{feedback_effort}"
                     feedback_position="{feedback_position}"
                     result="{result}"/>
      <JointPosition instance="m14"
                     max_effort="0"
                     max_velocity="180"
                     position="300"
                     tolerance="10"
                     feedback_effort="{feedback_effort}"
                     feedback_position="{feedback_position}"
                     result="{result}"/>
      <JointPosition instance="m12"
                     max_effort="0"
                     max_velocity="180"
                     position="160"
                     tolerance="10"
                     feedback_effort="{feedback_effort}"
                     feedback_position="{feedback_position}"
                     result="{result}"/>
      <Translate frame_id="base_link"
                 ignore_obstacles="false"
                 x="0.2"
                 error="{error}"
                 linear_velocity="0.3"/>
    </Sequence>
  </BehaviorTree>

  <!-- Description of Node Models (used by Groot) -->
  <TreeNodesModel>
    <Action ID="JointPosition"
            editable="true">
      <input_port name="instance"/>
      <input_port name="max_effort"
                  default="0"/>
      <input_port name="max_velocity"
                  default="180"/>
      <input_port name="position"/>
      <input_port name="tolerance"
                  default="10"/>
      <output_port name="feedback_effort"
                   default="{feedback_effort}"/>
      <output_port name="feedback_position"
                   default="{feedback_position}"/>
      <output_port name="result"
                   default="{result}"/>
    </Action>
    <Action ID="Move"
            editable="true">
      <input_port name="goal"/>
      <inout_port name="ignore_obstacles"
                  default="true"/>
      <input_port name="linear_velocity"
                  default="0.5"/>
      <input_port name="frame_id"
                  default="map"/>
      <input_port name="angular_velocity"
                  default="1.0"/>
      <input_port name="mode"
                  default="7"/>
    </Action>
    <Action ID="ScoreboardTask"
            editable="true">
      <input_port name="points"
                  default="0">points scored, can be negative</input_port>
      <input_port name="task"
                  default="store_sample_to_work_shed">unique task name</input_port>
    </Action>
    <Action ID="Translate"
            editable="true">
      <input_port name="frame_id"
                  default="base_link"/>
      <input_port name="ignore_obstacles"
                  default="true"/>
      <input_port name="x"/>
      <input_port name="error"
                  default="{error}"/>
      <input_port name="linear_velocity"
                  default="0.5"/>
    </Action>
  </TreeNodesModel>

</root>
