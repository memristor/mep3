<?xml version="1.0" encoding="UTF-8"?>
<root BTCPP_format="4">
  <BehaviorTree ID="try_pick_up_weed_back">
    <Sequence>
      <JointPosition instance="m12"
                     max_effort="0"
                     max_velocity="180"
                     position="177"
                     tolerance="10"
                     feedback_effort="{feedback_effort}"
                     feedback_position="{feedback_position}"
                     result="{result}"/>
      <Translate frame_id="base_link"
                 ignore_obstacles="true"
                 x="0.07"
                 error="{error}"
                 linear_velocity="0.5"/>
      <JointPosition instance="m12"
                     max_effort="0"
                     max_velocity="150"
                     position="70"
                     tolerance="10"
                     feedback_effort="{feedback_effort}"
                     feedback_position="{feedback_position}"
                     result="{result}"/>
      <Parallel failure_count="1"
                success_count="-1">
        <JointPosition instance="m14"
                       max_effort="0"
                       max_velocity="180"
                       position="0"
                       tolerance="10"
                       feedback_effort="{feedback_effort}"
                       feedback_position="{feedback_position}"
                       result="{result}"/>
        <Translate frame_id="base_link"
                   ignore_obstacles="true"
                   x="-0.25"
                   error="{error}"
                   linear_velocity="0.2"/>
      </Parallel>
      <JointPosition instance="m11"
                     max_effort="0"
                     max_velocity="180"
                     position="235"
                     tolerance="10"
                     feedback_effort="{feedback_effort}"
                     feedback_position="{feedback_position}"
                     result="{result}"/>
    </Sequence>
  </BehaviorTree>

  <BehaviorTree ID="try_pick_up_weed_front">
    <Sequence>
      <JointPosition instance="m3"
                     max_effort="0"
                     max_velocity="180"
                     position="205"
                     tolerance="10"
                     feedback_effort="{feedback_effort}"
                     feedback_position="{feedback_position}"
                     result="{result}"/>
      <Translate frame_id="base_link"
                 ignore_obstacles="true"
                 x="-0.07"
                 error="{error}"
                 linear_velocity="0.5"/>
      <Parallel failure_count="1"
                success_count="-1">
        <JointPosition instance="m3"
                       max_effort="0"
                       max_velocity="170"
                       position="85"
                       tolerance="10"
                       feedback_effort="{feedback_effort}"
                       feedback_position="{feedback_position}"
                       result="{result}"/>
        <Translate frame_id="base_link"
                   ignore_obstacles="true"
                   x="0.25"
                   error="{error}"
                   linear_velocity="0.2"/>
      </Parallel>
      <JointPosition instance="m2"
                     max_effort="0"
                     max_velocity="150"
                     position="140"
                     tolerance="10"
                     feedback_effort="{feedback_effort}"
                     feedback_position="{feedback_position}"
                     result="{result}"/>
    </Sequence>
  </BehaviorTree>

  <!-- Description of Node Models (used by Groot) -->
  <TreeNodesModel>
    <Action ID="JointPosition"
            editable="true">
      <input_port name="instance"/>
      <input_port name="max_effort"
                  default="0"/>
      <input_port name="max_velocity"
                  default="180"/>
      <input_port name="position"/>
      <input_port name="tolerance"
                  default="10"/>
      <output_port name="feedback_effort"
                   default="{feedback_effort}"/>
      <output_port name="feedback_position"
                   default="{feedback_position}"/>
      <output_port name="result"
                   default="{result}"/>
    </Action>
    <Action ID="Translate"
            editable="true">
      <input_port name="frame_id"
                  default="base_link"/>
      <input_port name="ignore_obstacles"
                  default="true"/>
      <input_port name="x"/>
      <input_port name="error"
                  default="{error}"/>
      <input_port name="linear_velocity"
                  default="0.5"/>
    </Action>
  </TreeNodesModel>

</root>
